    "version": "0.2.0",
    "configurations": [
        {
            "name": "GC: g explore",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${fileDirname}${pathSeparator}..${pathSeparator}examples",
            "program": ".${pathSeparator}global_coupling_fitting.py",
            "console": "integratedTerminal",
            "env": {"PYTHONPATH": "..${pathSeparator}src"},
            "args": [
                "--nsubj", "21",
                "--observables", "FC,KS,PS",
                "--model", "Deco2014", "--obs-var", "re", "--scale-signal", "1000.0",
                "--param", "list", "g", "1.0", "2.0", "4.0", "8.0",
                "--tr", "720",
                "--tmax", "600",
                "--fmri-path", "./Data_Raw/ebrains_popovych",
                "--out-path", "./Data_Produced/ebrains_popovych"
            ]
        },
        {
            "name": "GC: plot G results",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${fileDirname}${pathSeparator}..${pathSeparator}examples",
            "program": ".${pathSeparator}global_coupling_fitting.py",
            "console": "integratedTerminal",
            "env": {"PYTHONPATH": "..${pathSeparator}src"},
            "args": [
                "--plot-g",
                "--model", "Deco2014",
                "--tr", "720",
                "--observables", "swFCD", "FC", "phFCD",
                "--fmri-path", "./Data_Raw/ebrains_popovych",
                "--out-path", "./Data_Produced/ebrains_popovych"
            ]
        }
    ]
}